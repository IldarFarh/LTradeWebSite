<header>
	<nav class="navbar navbar-default" role="navigation">
	    <div class="container">
	      <div class="navbar-header">
	        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar_collapse">
	          <span class="sr-only">Toggle navigation</span>
	          <span class="icon-bar"></span>
	          <span class="icon-bar"></span>
	          <span class="icon-bar"></span>
	        </button>
	        <a class="navbar-brand" href="/"><img src="/images/logo/logo_small.png" class="img-responsive logo" alt="Лизинг-Трейд" align="center"></a>
	      </div> <!-- navbar-header -->
	      <div class="collapse navbar-collapse" id="navbar_collapse">
	        <ul class="nav navbar-nav navbar-right">
	          <li><a href="/"><i class="fa fa-home"></i> Главная</a></li>
				<% menuList.forEach(function(item) { %>
				<li class="dropdown">
		          	<a href="#" class="dropdown-toggle" data-toggle="dropdown"><%= item.title %> <span class="caret"></span></a>
		          	<ul class="dropdown-menu">
						<% item.sublevels.forEach(function(element) { %>			
							<li><a href="/<%= item.link %>/<%= element.link %>"><%= element.title %></a></li>
						<% }); %>	
					</ul>
		 		</li>
				<% }); %>
	          <li class="visible-sm visible-xs"><a href="/contactlist">Справочник</a></li>	
	        </ul>
	      </div><!-- collapse navbar-collapse -->
	    </div><!-- container -->
  	</nav>
</header>